:vpi_module "va_math";
S_0x1a71a70 .scope module, "test_pc_hold" "test_pc_hold" 2 3;
 .timescale 0 0;
v0x1a95160_0 .var "clk", 0 0;
v0x1a95230_0 .var "input_addr", 31 0;
v0x1a95300_0 .net "output_addr", 31 0, v0x1a94ff0_0;  1 drivers
S_0x1a71bf0 .scope module, "flippy" "pc_hold" 2 8, 3 2 0, S_0x1a71a70;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 32 "pc_in"
    .port_info 2 /OUTPUT 32 "pc_out"
P_0x1a724c0 .param/l "N" 0 3 2, +C4<00000000000000000000000000100000>;
v0x1a3bcf0_0 .net "clk", 0 0, v0x1a95160_0;  1 drivers
v0x1a94f10_0 .net "pc_in", 31 0, v0x1a95230_0;  1 drivers
v0x1a94ff0_0 .var "pc_out", 31 0;
E_0x1a74a00 .event posedge, v0x1a3bcf0_0;
    .scope S_0x1a71bf0;
T_0 ;
    %wait E_0x1a74a00;
    %load/vec4 v0x1a94f10_0;
    %assign/vec4 v0x1a94ff0_0, 0;
    %jmp T_0;
    .thread T_0;
    .scope S_0x1a71a70;
T_1 ;
    %pushi/vec4 4278255360, 0, 32;
    %assign/vec4 v0x1a95230_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x1a95160_0, 0;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x1a95160_0, 0;
    %delay 5, 0;
    %vpi_call 2 16 "$display", "output address %b", v0x1a95300_0 {0 0 0};
    %vpi_call 2 17 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
